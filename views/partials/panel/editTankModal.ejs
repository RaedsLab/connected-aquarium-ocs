<!-- MODAL EDIT TANK -->
<div class="modal" id="editTankModal" tabindex="-1" role="dialog">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">Ã—</span>
        </button>
        <h4 class="modal-title">Edit aquarium</h4>
      </div>
      <div class="modal-body">
        <div class="editTank">
          <form id="editTankForm">
            <label for="code">Code</label><br>
            <input disabled required="required" id="editTankCode" type="text">
            <br>
            <label for="label">Label</label><br>
            <input required="required" id="editTankLabel" type="text" placeholder="Give your tank a name">
            <br><br>
            <label for="latitude">Latitude</label>
            <input required="required" id="editTankLatitude" type="text" placeholder="0.0">

            <label for="longitude">Longitude</label>
            <input required="required" id="editTankLongitude" type="text" placeholder="0.0">
            <br>
          </form>
        </div>

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default pull-left" data-dismiss="modal">Cancel</button>
        <button onclick="editATank()" id="btnEditTank" type="button" class="btn btn-primary">Edit</button>
      </div>
    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
</div><!-- /.modal -->
<!-- / MODAL EDIT TANK -->
